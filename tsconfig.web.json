{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.css",
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ]
    }
  }
}

